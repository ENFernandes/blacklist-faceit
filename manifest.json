{
    "manifest_version": 3,
    "name": "BlackList",
    "description": "BlackList dos idiotas",
    "version": "1.0",
    "action":{
      "default_popup":"popup.html"
    },
    "permissions": [
      "activeTab",
      "storage"
    ],
    "content_scripts": [
      {
        "run_at": "document_end",
        "matches": [
          "https://www.faceit.com/*"
        ],
        "js": [
          "popup.js"
        ],
        "css":[
          "style.css"
        ]
      }
    ]
  }